<?xml version="1.0" encoding="UTF-8"?>
<!--Instituto Stela 2014-->
<!--Manual de Orientação do eSocial - Versão 1.1 - 06 de Janeiro de 2014-->
<!--ComplexTypes compartilhados pelos layouts do eSocial-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:include schemaLocation="Shared_Simple_Types.xsd"/>

    <xs:complexType name="TideEvento">
        <xs:sequence>
            <xs:element name="tpAmb" type="xs:integer" minOccurs="1" maxOccurs="1"/>
            <xs:element name="procEmi" type="xs:integer" minOccurs="1" maxOccurs="1"/>
            <xs:element name="indSegmento" type="xs:integer" minOccurs="1" maxOccurs="1"/>
            <xs:element name="verProc" type="xs:string" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TideEmpregador">
        <xs:sequence>
            <xs:element name="tpInscricao" type="xs:integer" minOccurs="1" maxOccurs="1"/>
            <xs:element name="nrInscricao" type="xs:string" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TidePeriodo">
        <xs:sequence>
            <xs:element name="iniValidade" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="fimValidade" type="xs:string" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Tcontato">
        <xs:sequence>
            <xs:element name="nomeContato" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="cpfContato" type="Tcpf" minOccurs="1" maxOccurs="1"/>
            <xs:element name="foneFixo" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="foneCelular" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="fax" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>